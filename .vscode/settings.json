{
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "editor.fontSize": 16,
    "workbench.commandPalette.experimental.suggestCommands": true,
    "workbench.colorTheme": "Purple-Codain",
    "workbench.preferredDarkColorTheme": "Default Dark Modern",
    "bbtag.trace.server": "verbose",
    "bbtag.maxNumberOfProblems": 100,
    "editor.defaultFormatter": "blargbot.bbtag",
      "files.autoSave": "onWindowChange",
      "material-icon-theme.activeIconPack": "none",
      "material-icon-theme.files.color": "#C5C4F9",
      "material-icon-theme.folders.color": "#956CB2",
      "material-icon-theme.folders.theme": "classic",
      "material-icon-theme.hidesExplorerArrows": true
}

